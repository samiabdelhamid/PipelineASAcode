pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'hello'
      }
    }

    stage('test') {
      steps {
        echo 'test start'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy completed'
      }
    }

    stage('commit') {
      steps {
        echo 'commit completed'
      }
    }

  }
}